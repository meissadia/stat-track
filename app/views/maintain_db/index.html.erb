<main>
  <section id="buttons">
    <h1>Update Database</h1>
    <h2>Last Update: <%= @last_update %></h2>
    <div class=center>
      <p><%= link_to 'Update DB', '/maintain_db/updateDb', method: :post %></p>
    </div>
  </section>
  <section id=status>
    <% @status.each do |msg| %>
      <% if !msg.empty? %>
        <p><%= msg %></p>
      <% end %>
    <% end %>
  </section>
</main>
